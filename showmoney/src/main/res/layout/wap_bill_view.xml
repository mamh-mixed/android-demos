<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:cardinfolink="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.cardinfolink.yunshouyin.ui.SettingActionBarItem
        android:id="@+id/action_bar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        cardinfolink:left_text="@string/setting_return"
        cardinfolink:title="@string/setting_my_web_yunshouyin" />


    <WebView
        android:id="@+id/base_webview"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scrollbarSize="0dip" />

    <include layout="@layout/showmoney_dialog"></include>

</LinearLayout>
